#include<iostream>
using namespace std;

int Partition(int A[], int left, int right)
{
	int temp = A[left];
	while(left < right){
		while(left < right) if(A[right] >= temp) right--;
		A[left] = A[right];
		while(left < right) if(A[left] < temp) left++;
		A[right] = A[left];
	}
	A[left] = temp;
	return left;
}

void QuickSort(int A[], int left, int right)
{
	if(left > right)
		return;
	int Position = Partition(A, left, right);
	QuickSort(A,left,Position-1);
	QuickSort(A,Position+1,right);
}

int main()
{
	int A[7] = {5,3,4,7,5,9,1};
	QuickSort(A, 0, 6);
	
	for(int i = 0; i < 7; i++){
		printf("%d", A[i]);
		if(i != 6)
			putchar(' ');
	}
	
	system("pause");
	return 0;
}
